{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "declaration": true,
    "sourceMap": true,
    "jsx": "react-jsx",
    "lib": ["dom", "es2020"]
  }
}
